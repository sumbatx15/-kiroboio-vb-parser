{
  "name": "@kiroboio/vb-parser",
  "version": "1.0.0",
  "author": "Sumbatx15",
  "license": "MIT",
  "description": "A typescript boilerplate using swc to release a package on npm",
  "type": "commonjs",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sumbatx15/visual-builder-parser.git"
  },
  "bugs": {
    "url": "https://github.com/sumbatx15/visual-builder-parser/issues"
  },
  "homepage": "https://github.com/sumbatx15/visual-builder-parser#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "boilerplate",
    "typescript",
    "swc",
    "package",
    "release",
    "github"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@babel/generator": "^7.21.9",
    "@babel/types": "^7.21.5",
    "lodash-es": "^4.17.21"
  },
  "resolutions": {
    "@babel/generator": "^7.21.9",
    "@babel/types": "^7.21.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.0",
    "@semantic-release/release-notes-generator": "^11.0.0",
    "@swc-node/register": "1.6.4",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.52",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "18.15.12",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "5.0.0",
    "semantic-release": "^21.0.2",
    "typescript": "5.0.4"
  }
}
